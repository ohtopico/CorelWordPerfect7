                         %bodyatt;
                         %secur;                               >

     <!ELEMENT contents              - o       EMPTY           >
     <!ATTLIST contents  shortentry     %yesorno;      "0"     >

     <!ELEMENT (illuslist|
      tablelist)                     - o       EMPTY           >
     <!ATTLIST (illuslist|
      tablelist)         tocentry       %yesorno;      "1"
                         shortentry     %yesorno;      "0">

     <!ELEMENT safesum               - o     (para | precaut |
